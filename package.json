{
  "name": "gatsby-starter-minimal",
  "version": "1.0.0",
  "private": true,
  "description": "A Gatsby starter with no plugins and a single welcome page.",
  "author": "Laurie Barth",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "start": "gatsby build; gatsby serve --port $PORT",
    "dev": "gatsby develop",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "poll-content": "while sleep 1; do curl -X POST http://localhost:8000/__refresh; done",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js"
  },
  "license": "0BSD",
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@fontsource/manrope": "^4.5.10",
    "@fontsource/open-sans": "^4.5.10",
    "bootstrap": "^4.6.0",
    "gatsby": "^4.20.0",
    "gatsby-plugin-emotion": "^7.20.0",
    "gatsby-plugin-google-tagmanager": "^4.20.0",
    "gatsby-plugin-image": "^2.20.0",
    "gatsby-plugin-manifest": "^4.20.0",
    "gatsby-plugin-mixpanel": "^3.7.0",
    "gatsby-plugin-netlify": "^5.0.1",
    "gatsby-plugin-offline": "^5.20.0",
    "gatsby-plugin-purgecss": "^6.1.2",
    "gatsby-plugin-react-helmet": "^5.20.0",
    "gatsby-plugin-remove-trailing-slashes": "^4.19.0",
    "gatsby-plugin-sharp": "^4.20.0",
    "gatsby-plugin-sitemap": "^5.20.0",
    "gatsby-source-cosmicjs": "^1.2.1",
    "gatsby-source-filesystem": "^4.20.0",
    "gatsby-transformer-sharp": "^4.20.0",
    "jquery": "^3.6.0",
    "lazysizes": "^5.3.2",
    "lodash": "^4.17.21",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "eslint": "^8.21.0",
    "eslint-plugin-react": "^7.30.1",
    "prettier": "^2.7.1"
  }
}
